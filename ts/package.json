{
  "name": "obsidian_scripts",
  "version": "0.1.0",
  "description": "All the plugin scripts I use in my Obsidian vaults.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx tsc --project tsconfig.build.json",
    "bundle:tasks": "npx webpack --config webpack.tasks.config.js",
    "bundle:commands": "npx webpack --config webpack.commands.config.js",
    "bundle:dv": "npx webpack --config webpack.testView.config.js",
    "test": "npm run build && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petitpandarouge/ObsidianScripts.git"
  },
  "keywords": [
    "obsidian",
    "dataview",
    "quickadd",
    "userplugins"
  ],
  "author": "Damien Port√©",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petitpandarouge/ObsidianScripts/issues"
  },
  "homepage": "https://github.com/petitpandarouge/ObsidianScripts#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "mergician": "^2.0.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-jest": "^0.0.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
