{
  "name": "obsidian_scripts",
  "version": "0.1.0",
  "description": "All the plugin scripts I use in my Obsidian vaults.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./bundles",
    "clear": "npm run clean && rimraf ./node_modules && rimraf ./package-lock.json",
    "build": "npm run clean && npx tsc --project tsconfig.build.json",
    "rebuild": "npm run clean && npm install && npm run build",
    "bundle:tasks": "npx webpack --config webpack.tasks.config.js",
    "bundle:dv": "npx webpack --config webpack.testView.config.js && node dv.view.init.js",
    "bundle:quickadd": "npx webpack --config webpack.testQuickAdd.config.js",
    "bundle:userplugins": "npx webpack --config webpack.userplugins.config.js",
    "bundle": "npm run bundle:tasks && npm run bundle:quickadd && npm run bundle:dv",
    "test": "npm run build && jest",
    "ci": "npm run test && npm run bundle",
    "deploy:test": "ncp ./bundles c:/Temp/obsidian",
    "bundle:userplugins:helloworld": "npx webpack --config webpack.userplugins.helloworld.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petitpandarouge/ObsidianScripts.git"
  },
  "keywords": [
    "obsidian",
    "dataview",
    "quickadd",
    "userplugins"
  ],
  "author": "Damien Port√©",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petitpandarouge/ObsidianScripts/issues"
  },
  "homepage": "https://github.com/petitpandarouge/ObsidianScripts#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/chance": "^1.1.6",
    "@types/jest": "^29.5.12",
    "chance": "^1.1.11",
    "jest": "^29.7.0",
    "mergician": "^2.0.2",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-jest": "^0.0.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
